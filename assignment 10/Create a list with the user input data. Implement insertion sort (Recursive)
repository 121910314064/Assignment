##################################################################################################################
def N(Neh,ne): 
	
	if ne<=1: 
		return
	
	
	N(Neh,ne-1) 
	last = Neh[ne-1] 
	j = ne-2
	
	
	while (j>=0 and Neh[j]>last): 
		Neh[j+1] = Neh[j] 
		j = j-1

	Neh[j+1]=last 
	

def Ne(Neh,ne): 
	for i in range(ne): 
		print (Neh[i]), 
Neh = [87,95,21] 
ne = len(Neh) 
N(Neh, ne)
print("The elementing after sorting is:")
Ne(Neh, ne) 
###########################################################################################################################
output:
###########################################################################################################################
The elementing after sorting is:
21
87
95
###########################################################################################################################
